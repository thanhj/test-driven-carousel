{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "package-lock.json": true
  },
  "eslint.run": "onSave",
  "prettier.eslintIntegration": true,
  "[javascript]": {
    "editor.tabSize": 2
  },
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "editor.formatOnSave": true
}
